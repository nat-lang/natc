class Object < __obj__ {
  init() => {
    this.hash = nil;
  }

  __get__(key) => {
    if (type(key).sub(Object)) key.setHash();

    return this.get(key);
  }

  __set__(key, value) => {
    if (type(key).sub(Object)) key.setHash();

    this.set(key, value);

    return this;
  }

  __in__(value) => {
    if (type(value).sub(Object)) value.setHash();

    return this.has(value);
  }

  setHash() => {
    this.hash = this.__hash__();
  }

  __hash__() => {}
}

