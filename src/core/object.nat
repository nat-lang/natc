

class Object < __obj__ {
  // wrap the native sequences in sequence instances.
  entries() => {
    let seq = [];

    for (entry in entries(this))
      seq.add([entry[0], entry[1]]);

    return seq;
  }
  keys() => [entry[0] | entry in entries(this)]
  values() => [entry[1] | entry in entries(this)]
}
