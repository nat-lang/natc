class Sequential extends Object {
  __get__(idx) => this.values[idx];
  __set__(idx, val) => {
    this.values[idx] = val;
    return this;
  }
  __len__() => len(this.values);
  __eq__(seq) => {
    if (len(this) != len(seq)) return false;

    for (let i = 0; i < len(this); i = i + 1) {
      if (this[i] != seq[i])
        return false;
    }
    return true;
  }

  __in__(value) => {
    for (x in this)
      if (value == x) return true;

    return false;
  }

  hash() => this.map(hash).reduce(+, 0);

  map(fn) => [fn(value) | value in this];

  reduce(fn, acc) => {
    for (x in this)
      acc = fn(acc, x);

    return acc;
  }

  filter(predicate) => [x | x in this, !predicate(x)];

  concat(other) => {
    let seq = [x | x in this];
    for (x in other) seq.push(x);
    return seq;
  }

  all(predicate) => this.map(predicate).reduce((x,y) => x and y, true);
  some(predicate) => this.map(predicate).reduce((x,y) => x or y, false);

  pp() => {
    print this.values;
  }

  fromRaw(raw) => {
    let seq = Sequence();
    for (let i = 0; i < len(raw); i = i + 1)
      seq.push(raw[i]);
    return seq;
  }
}

class Tuple extends Sequential {}

class Sequence extends Sequential {
  extend(elements) => {
    for (x in elements)
      this.push(x);
  }
  last() => this.values[len(this) - 1];
}
