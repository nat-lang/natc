class Type {}

class Metaclass < Type {
  isSubclass(klass) => {
    while (klass) {
      // pointer equality so that we can 
      // define `==` in terms of subclass.
      if (klass is this) return true;

      klass = klass.superclass;
    }
    return false;
  }

  __eq__(klass) => this.isSubclass(klass) or klass.isSubclass(this)
}
