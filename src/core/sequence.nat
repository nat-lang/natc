
class Sequence < __seq__ {
  __get__(idx) => this.values[idx]
  __set__(idx, val) => {
    this.values[idx] = val;
    return this;
  }
  __len__() => len(this.values)

  // The cartesian product of [this] x [other].
  product(other) => {
    let seq = [];

    for (x in this) {
      for (y in other) {
        seq.add([x, y]);
      }
    }

    return seq;
  }
}
