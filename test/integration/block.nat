// lexical scope.

let a = 0;
assert(a == 0);

{
  assert(a == 0);
  let a = 1;
  assert(a == 1);

  {
    assert(a == 1);
    let a = 2;
    assert(a == 2);

    a = 3;
    assert(a == 3);
  }

  assert(a == 1);
  a = 4;
  assert(a == 4);
}

assert(a == 0);
a = 5;
assert(a == 5);


let f = () => {
  print c;
};

let c = 1;

f();