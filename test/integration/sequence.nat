
let seqInstance = [1,2,3];

// the underlying seq object itself.
let seq = seqInstance.values;

// subscript access.
assert(seq[0] == 1);
assert(seq[2] == 3);

// subscript assignment.
seq[0] = 2;
assert(seq[0] == 2);
seq[2] = 0;
assert(seq[2] == 0);

