let a = "a";
let b = "b";

// identity.

assert(a != b);

// concatenation.

assert(a + b == "ab");

let x = str(1) + str(2);

assert(x == "12");

// length.

assert(len("a") == 1);
assert(len("ab") == 2);

// indexing.

assert("abc"[0] == "a");
assert("abc"[1] == "b");
assert("abc"[2] == "c");

let x = "abc";

x[0] = "c";
x[2] = "a";

assert(x == "cba");


