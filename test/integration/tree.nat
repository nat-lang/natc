let tree;

// literal trees.

tree = [1 [2 3 4] [5 6 7]];

assert(tree.data == 1);

assert(tree.children[0].data == 2);
assert(tree.children[1].data == 5);

tree = [true false false];

assert(tree.data);
assert(!tree.children[0] and !tree.children[1]);

// expression trees.

tree = [(1 + 1) (1 + 2) (1 + 3)];

assert(tree.data == 2);
assert(tree.children[0] == 3);
assert(tree.children[1] == 4);

let id = (a) => { return a; };

tree = [id(1) id(2) id(3)];

assert(tree.data == 1);
assert(tree.children[0] == 2);
assert(tree.children[1] == 3);

