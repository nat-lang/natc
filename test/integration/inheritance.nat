
// override method.

class A {
  begin() => {
    return 1;
  }

  conclude() => {
    return 2;
  }

  compose() => {
    return this.begin() + this.conclude();
  }
}

class B extends A {
  conclude() => {
    return 1;
  }
}

assert(B().compose() == 2);

// resolve super call in a parent.

class A {
  method() => {
    return "A";
  }
}

class B extends A {
  method() => {
    return "B";
  }

  test() => {
    return super.method();
  }
}

class C extends B {}

assert(C().method() == "B");
assert(C().test() == "A");

// close over a super method.

class A {
  method() => {
    return "A";
  }
}

class B extends A {
  method() => {
    let closure = super.method;
    
    return closure;
  }
}

assert(B().method()() == "A");
